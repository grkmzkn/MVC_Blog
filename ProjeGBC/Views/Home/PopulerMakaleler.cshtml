@model IEnumerable<ProjeGBC.Models.Makale>

@{
    Layout = null;
}


<div class="row">
    <div class="col-lg-12">
        <h4 class="card-header">En Çok Okunan Makaleler</h4>
        <div class="col-lg-12">
            <ul class="list-unstyled mb-0">
                @foreach (var item in Model)
                {
                    <li><a href="/Home/MakaleDetay/@item.MakaleId">@item.Baslik </a> (@item.Okunma)</li>
                }
            </ul>
        </div>
    </div>
</div>